{
  "name": "Nates Facebook Suppressor",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Removes the stuff I don't like from Facebook",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": [
        "execute.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ]
}
